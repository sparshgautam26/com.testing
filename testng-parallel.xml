<!DOCTYPE suite SYSTEM
"https://testng.org/testng-1.0.dtd" >

<suite name="ParallelSuite"
       parallel="methods"
       thread-count="3">

    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>

    <test name="AllTests">
        <classes>
            <class name="tests.LoginTest"/>
            <class name="tests.RaceTest"/>
        </classes>
    </test>

</suite>
